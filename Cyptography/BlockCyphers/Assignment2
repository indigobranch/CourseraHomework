from Crypto.Cipher import AES
from Crypto import Random


key = "140b41b22a29beb4061bda66b6747e14"
mode = AES.MODE_CBC
cipherText = b"4ca00ff4c898d61e1edbf1800618fb2828a226d160dad07883d04e008a7897ee2e4b7465d5290d0c0e6c6822236e1daafb94ffe0c5da05d9476be028ad7c1d81".decode("hex")

iv = Random.new().read(AES.block_size)
cipher = AES.new(key, mode, iv)
plain = cipher.decrypt(cipherText)

print(plain)

#    Changes a full hex string to an ascii string
def hexStringToString(hexString):

    return hexString
